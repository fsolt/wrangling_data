	journal = {The Journal of Politics},
	number = {3},
	pages = {685-693},
	title = {But Wait, There's More! Maximizing Substantive Inferences from TSCS Models},
	url = {https://doi.org/10.1017/S0022381612000473},
	volume = {74},
	year = {2012},
